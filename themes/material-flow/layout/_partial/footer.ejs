<footer id="footer" class="clearfix">

	<div class="social-wrapper">
  	<% if (theme.social) { %>
      <% theme.social.forEach(function(value, key) {
           if (value.url && value.slug) { %>
        <a href="<%= value.url %>" class="social <%= value.slug %>"
          target="_blank" rel="external">
          <span class="fa fa-<%= value.slug %>"></span>
        </a>
      <% } }); %>
    <% } %>
  </div>
  
  <div>
    Â© 2017 <%= config.author %>
  </div>
  <div>
    Powered by <a href='https://hexo.io/' class="codename">Hexo</a> - Theme <a href='https://github.com/stkevintan/hexo-theme-material-flow' class="codename">MaterialFlow</a>
  </div>
  <div>
    <span id="visits">Visits: <font class="count">--<font></span>
  </div>
  
</footer>

